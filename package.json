{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "start": "node src/server.ts",
    "lint": "eslint . --ext .ts",
    "prettier": "prettier --config .prettierrc \"src/**/*.ts\" --write",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess 'node ./dist/server.js'",
    "tsc": "tsc",
    "migrate": "db-migrate --env test up && db-migrate up",
    "test": "set ENV=test && db-migrate --env test up && jasmine-ts && db-migrate --env test down",
    "jasmine-ts-users": "jasmine-ts spec/tests/models/usersSpec.ts",
    "jasmine-ts-categories": "jasmine-ts spec/tests/models/categoriesSpec.ts",
    "jasmine-ts-products": "jasmine-ts spec/tests/models/productsSpec.ts",
    "jasmine-ts-orders": "jasmine-ts spec/tests/models/ordersSpec.ts"
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.9",
    "@types/pg": "^7.14.7",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "db-migrate": "^0.11.12",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.5.1",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/jasmine": "^3.6.3",
    "@types/jsonwebtoken": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jasmine": "^3.6.4",
    "jasmine-spec-reporter": "^6.0.0",
    "jasmine-ts": "^0.3.0",
    "prettier": "^2.3.2",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9"
  }
}
