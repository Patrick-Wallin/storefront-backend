{
  "spec_dir": "./dist/spec/tests",
  "spec_files": [
    "handlers/*[sS]pec.js",
    "models/*[sS]pec.js"
  ],
  "helpers": [
    "helpers/**/*.js"
  ],
  "reporters": [
    {
      "name": "jasmine-spec-reporter#SpecReporter",
      "options": {
        "displayStacktrace": "all"
      }
    }
  ],
  "stopSpecOnExpectationFailure": false,
  "random": false
}
